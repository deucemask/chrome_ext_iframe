{
  "name": "iFrame Remover",
  "description": "Removes iframes from current page",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["rm.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Remove iframes",
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon_48.png"
  },
  "manifest_version": 2
}
